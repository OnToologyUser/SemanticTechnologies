        <owl:disjointWith rdf:resource="http://www.owl-ontologies.com/base#DramaticExpression"/>
        <rdfs:comment>Non literary expression  whose aim is to study or analyse a dramatic expression</rdfs:comment>
        <rdfs:label>Academic Expression</rdfs:label>
    </owl:Class>
    


