                                        owl:inverseOf <http://purl.org/spar/frbr/#embodimentOf> ;
                                        rdfs:domain <http://purl.org/spar/frbr/#expression> ;
                                        rdfs:range <http://purl.org/spar/frbr/#manifestation> ;
                                        rdfs:comment "A particular expression is found in a certain manifestation" ;
