#################################################################
#    Object Properties
#################################################################

###  http://purl.org/spar/frbr/#embodiment
frbr:embodiment rdf:type owl:ObjectProperty ;
                owl:inverseOf frbr:embodimentOf ;
                rdfs:domain frbr:expression ;
                rdfs:range frbr:manifestation ;
                rdfs:comment "A particular expression is found in a certain manifestation" ;
                rdfs:label "embodiment" .


###  http://purl.org/spar/frbr/#embodimentOf
frbr:embodimentOf rdf:type owl:ObjectProperty ;
                  rdfs:domain frbr:manifestation ;
                  rdfs:range frbr:expression ;
                  rdfs:comment "Manifestation is an embodiment of an expression" ;
                  rdfs:label "embodiment of" .


###  http://purl.org/spar/frbr/#has_creator
frbr:has_creator rdf:type owl:ObjectProperty ;
                 owl:equivalentProperty base:HasAuthor ;
                 owl:inverseOf frbr:isCreatorOf ;
                 rdfs:comment "A work was created by an Author" ;
                 rdfs:label "has_creator" .


###  http://purl.org/spar/frbr/#has_exemplar
frbr:has_exemplar rdf:type owl:ObjectProperty ;
                  owl:inverseOf frbr:is_exemplar_of ;
                  rdfs:domain frbr:manifestation ;
                  rdfs:range frbr:item ;
                  rdfs:comment "A manifestation has an exemplar" ;
                  rdfs:label "has_exemplar" .


###  http://purl.org/spar/frbr/#has_realization
frbr:has_realization rdf:type owl:ObjectProperty ;
                     owl:inverseOf frbr:is_realization_of ;
                     rdfs:domain frbr:work ;
                     rdfs:range frbr:expression ;
                     rdfs:comment "A work is realizated by an expression" ;
                     rdfs:label "has_realization" .


###  http://purl.org/spar/frbr/#isCreatorOf
frbr:isCreatorOf rdf:type owl:ObjectProperty ;
                 owl:equivalentProperty base:IsAuthorOf ;
                 rdfs:domain frbr:work ;
                 rdfs:range base:Author ;
                 rdfs:comment "An author created a work" ;
                 rdfs:label "is creator of" .


###  http://purl.org/spar/frbr/#is_exemplar_of
frbr:is_exemplar_of rdf:type owl:ObjectProperty ;
                    rdfs:domain frbr:item ;
                    rdfs:range frbr:manifestation ;
                    rdfs:comment "An item is exemplar of a manifestation" ;
                    rdfs:label "is_exemplar_of" .


###  http://purl.org/spar/frbr/#is_realization_of
frbr:is_realization_of rdf:type owl:ObjectProperty ;
                       rdfs:domain frbr:expression ;
                       rdfs:range frbr:work ;
                       rdfs:comment "An expression is realization of a work" ;
                       rdfs:label "is_realization_of" .


###  http://www.owl-ontologies.com/base#ActContainsScene
base:ActContainsScene rdf:type owl:ObjectProperty ;
                      owl:inverseOf base:SceneBelongsToAct ;
                      rdfs:domain base:Act ;
                      rdfs:range base:Scene ;
                      rdfs:comment "A particular scene belongs to a particular act" ;
                      rdfs:label "act contains scene" .


###  http://www.owl-ontologies.com/base#CommentsOn
base:CommentsOn rdf:type owl:ObjectProperty ;
                owl:inverseOf base:HasCriticalStudy ;
                rdfs:domain base:CriticalStudy ;
                rdfs:range base:DramaticExpression ;
                rdfs:comment "the critical study provides information about a particualr dramatic expression" ;
                rdfs:label "comments on" .


###  http://www.owl-ontologies.com/base#ContainsAct
base:ContainsAct rdf:type owl:ObjectProperty ;
                 owl:inverseOf base:act_belongs_to_dramatic_expression ;
                 rdfs:domain base:DramaticExpression ;
                 rdfs:range base:Act ;
                 rdfs:comment "A particular dramatic expression contains an act" ;
                 rdfs:label "contains act" .


###  http://www.owl-ontologies.com/base#Edits
base:Edits rdf:type owl:ObjectProperty ;
           owl:inverseOf base:IsEditedBy ;
           rdfs:domain base:EditingEntity ;
           rdfs:range frbr:expression ;
           rdfs:comment "An editing entity edits an expression" ;
           rdfs:label "edits" .


###  http://www.owl-ontologies.com/base#HasAccentualAnalysis
base:HasAccentualAnalysis rdf:type owl:ObjectProperty ;
                          owl:inverseOf base:IsAccentualAnalysis ;
                          rdfs:domain base:Verse ;
                          rdfs:range base:AccentAnalysis ;
                          rdfs:comment "A verse has accentual analysis" ;
                          rdfs:label "has accentual analysis" .


###  http://www.owl-ontologies.com/base#HasAuthor
base:HasAuthor rdf:type owl:ObjectProperty ;
               owl:inverseOf base:IsAuthorOf ;
               rdfs:domain frbr:work ;
               rdfs:range base:Author .


###  http://www.owl-ontologies.com/base#HasCharacter
base:HasCharacter rdf:type owl:ObjectProperty ;
                  owl:inverseOf base:IsCharacterIn ;
                  rdfs:domain base:DramaticExpression ;
                  rdfs:range base:Character ;
                  rdfs:comment "A dramatic expression has a certain character" ;
                  rdfs:label "has character" .


###  http://www.owl-ontologies.com/base#HasCriticalStudy
base:HasCriticalStudy rdf:type owl:ObjectProperty ;
                      rdfs:domain base:DramaticExpression ;
                      rdfs:range base:CriticalStudy ;
                      rdfs:comment "Academic expression associated to a particular dramatic expression" ;
                      rdfs:label "has critical study" .


###  http://www.owl-ontologies.com/base#HasMaritalStatus
base:HasMaritalStatus rdf:type owl:ObjectProperty ;
                      owl:inverseOf base:IsMaritalStatusOf ;
                      rdfs:domain skos1:MaritalStatus ;
                      rdfs:range base:Character ;
                      rdfs:comment "A characters has martial status" ;
                      rdfs:label "has marital status" .


###  http://www.owl-ontologies.com/base#HasMetricStructure
base:HasMetricStructure rdf:type owl:ObjectProperty ;
                        owl:inverseOf base:IsMetricStructureOf ;
                        rdfs:domain base:MetricEntity ;
                        rdfs:range base:Metre ;
                        rdfs:comment "A metric entity has a metric structure" ;
                        rdfs:label "has metric structure" .


###  http://www.owl-ontologies.com/base#HasRole
base:HasRole rdf:type owl:ObjectProperty ;
             owl:inverseOf base:IsRoleOf ;
             rdfs:domain base:Character ;
             rdfs:range base:SocialRole ;
             rdfs:comment "A character has role" ;
             rdfs:label "has role" .


###  http://www.owl-ontologies.com/base#IsAccentualAnalysis
base:IsAccentualAnalysis rdf:type owl:ObjectProperty ;
                         rdfs:domain base:AccentAnalysis ;
                         rdfs:range base:Verse ;
                         rdfs:comment "A certain accent analysis is analysis of a verse" ;
                         rdfs:label "is accentual analysis" .


###  http://www.owl-ontologies.com/base#IsAuthorOf
base:IsAuthorOf rdf:type owl:ObjectProperty ;
                rdfs:domain base:Author ;
                rdfs:range frbr:work .


###  http://www.owl-ontologies.com/base#IsCharacterIn
base:IsCharacterIn rdf:type owl:ObjectProperty ;
                   rdfs:domain base:Character ;
                   rdfs:range base:DramaticExpression ;
                   rdfs:comment "A character appears in a Dramatic Expression" ;
                   rdfs:label "is character in" .


###  http://www.owl-ontologies.com/base#IsEditedBy
base:IsEditedBy rdf:type owl:ObjectProperty ;
                rdfs:domain frbr:expression ;
                rdfs:range base:EditingEntity ;
                rdfs:comment "An expression is edited by an editing entity" ;
                rdfs:label "is edited by" .


###  http://www.owl-ontologies.com/base#IsMaritalStatusOf
base:IsMaritalStatusOf rdf:type owl:ObjectProperty ;
                       rdfs:domain skos1:MaritalStatus ;
                       rdfs:range base:Character ;
                       rdfs:comment "A character has a marital status" ;
                       rdfs:label "is marital status of" .


###  http://www.owl-ontologies.com/base#IsMetricStructureOf
base:IsMetricStructureOf rdf:type owl:ObjectProperty ;
                         rdfs:domain base:Metre ;
                         rdfs:range base:MetricEntity ;
                         rdfs:comment "A certain metre describe the metric structure of a metric entity" ;
                         rdfs:label "is metric structure of" .


###  http://www.owl-ontologies.com/base#IsObtainedThroughSistemAnalysis
base:IsObtainedThroughSistemAnalysis rdf:type owl:ObjectProperty ;
                                     owl:inverseOf base:SystemProduceAnalysis ;
                                     rdfs:domain base:AccentAnalysis ;
                                     rdfs:range base:AccentAnalysisSystem ;
                                     rdfs:comment "An accent analysis was created using a particular accent system and that information is critical to interpret it" ;
                                     rdfs:label "is obtained through system analysis" .


###  http://www.owl-ontologies.com/base#IsReferencedBy
base:IsReferencedBy rdf:type owl:ObjectProperty ;
                    owl:inverseOf base:References ;
                    rdfs:domain base:AcademicExpression ;
                    rdfs:range base:AcademicExpression ;
                    rdfs:comment "An Academic Expression is referenced by another Academic Expression" ;
                    rdfs:label "is referenced by" .


###  http://www.owl-ontologies.com/base#IsRoleOf
base:IsRoleOf rdf:type owl:ObjectProperty ;
              rdfs:domain base:SocialRole ;
              rdfs:range base:Character ;
              rdfs:comment "A certain role is associated to a character" ;
              rdfs:label "is role of" .


###  http://www.owl-ontologies.com/base#IsStatedBy
base:IsStatedBy rdf:type owl:ObjectProperty ;
                owl:inverseOf base:States ;
                rdfs:domain base:Verse ;
                rdfs:range base:Character ;
                rdfs:comment "A verse is stated by one or more characters" ;
                rdfs:label "is stated by" .


###  http://www.owl-ontologies.com/base#MetricEntityContainsVerse
base:MetricEntityContainsVerse rdf:type owl:ObjectProperty ;
                               owl:inverseOf base:VerseBelongsToMetricEntity ;
                               rdfs:domain base:MetricEntity ;
                               rdfs:range base:Verse ;
                               rdfs:comment "A metric entity contains 1 or more verses" ;
                               rdfs:label "metric entity contains verse" .


###  http://www.owl-ontologies.com/base#NoteReferencesVerse
base:NoteReferencesVerse rdf:type owl:ObjectProperty ;
                         owl:inverseOf base:VerseIsReferenciatedInNote ;
                         rdfs:domain base:Note ;
                         rdfs:range base:Verse ;
                         rdfs:comment "A particular note references a verse of the dramatic expression and provides information about it" ;
                         rdfs:label "note references verse" .


###  http://www.owl-ontologies.com/base#References
base:References rdf:type owl:ObjectProperty ;
                rdfs:domain base:AcademicExpression ;
                rdfs:range base:AcademicExpression ;
                rdfs:comment "An academic expression references another academic expression to provide insigths on information sources" ;
                rdfs:label "references" .


###  http://www.owl-ontologies.com/base#SceneBelongsToAct
base:SceneBelongsToAct rdf:type owl:ObjectProperty ;
                       rdfs:domain base:Scene ;
                       rdfs:range base:Act ;
                       rdfs:comment "A particular scene belongs to an act" ;
                       rdfs:label "scene belongs to act" .


###  http://www.owl-ontologies.com/base#SceneContainsVerse
base:SceneContainsVerse rdf:type owl:ObjectProperty ;
                        owl:inverseOf base:VerseBelongsToScene ;
                        rdfs:domain base:Scene ;
                        rdfs:range base:Verse ;
                        rdfs:comment "A particular scene contains a verse" ;
                        rdfs:label "scene contains verse" .


###  http://www.owl-ontologies.com/base#SceneHasStageDir
base:SceneHasStageDir rdf:type owl:ObjectProperty ;
                      owl:inverseOf base:StageDirBelongsToScene ;
                      rdfs:domain base:Scene ;
                      rdfs:range base:StageDirection ;
                      rdfs:comment "An scene contains stage dir that provide non-linguistic information" ;
                      rdfs:label "scene has stage dir" .


###  http://www.owl-ontologies.com/base#StageDirBelongsToScene
base:StageDirBelongsToScene rdf:type owl:ObjectProperty ;
                            rdfs:domain base:StageDirection ;
                            rdfs:range base:Scene ;
                            rdfs:comment "An stage dir belongs to a particular scene" ;
                            rdfs:label "stage dir belongs to scene" .


###  http://www.owl-ontologies.com/base#States
base:States rdf:type owl:ObjectProperty ;
            rdfs:domain base:Character ;
            rdfs:range base:Verse ;
            rdfs:comment "A character states a verse" ;
            rdfs:label "states" .


###  http://www.owl-ontologies.com/base#SystemProduceAnalysis
base:SystemProduceAnalysis rdf:type owl:ObjectProperty ;
                           rdfs:domain base:AccentAnalysisSystem ;
                           rdfs:range base:AccentAnalysis ;
                           rdfs:comment "An accent system produces an accent analysis" ;
                           rdfs:label "system produce analysis" .


###  http://www.owl-ontologies.com/base#TextualReference
base:TextualReference rdf:type owl:ObjectProperty ;
                      rdfs:domain base:AcademicExpression ,
                                  base:Note ;
                      rdfs:range base:Note .


###  http://www.owl-ontologies.com/base#VerseBelongsToMetricEntity
base:VerseBelongsToMetricEntity rdf:type owl:ObjectProperty ;
                                rdfs:domain base:Verse ;
                                rdfs:range base:MetricEntity ;
                                rdfs:comment "A verse belongs to a particular metric entity" ;
                                rdfs:label "verse belongs to metric entity" .


###  http://www.owl-ontologies.com/base#VerseBelongsToScene
base:VerseBelongsToScene rdf:type owl:ObjectProperty ;
                         rdfs:domain base:Verse ;
                         rdfs:range base:Scene ;
                         rdfs:comment "A verse belongs to a particular scene" ;
                         rdfs:label "verse belongs to scene" .


###  http://www.owl-ontologies.com/base#VerseIsReferenciatedInNote
base:VerseIsReferenciatedInNote rdf:type owl:ObjectProperty ;
                                rdfs:domain base:Verse ;
                                rdfs:range base:Note ;
                                rdfs:comment "A verse is reerenciated in a note" ;
                                rdfs:label "verse is referenciated in note" .


###  http://www.owl-ontologies.com/base#act_belongs_to_dramatic_expression
base:act_belongs_to_dramatic_expression rdf:type owl:ObjectProperty ;
                                        rdfs:domain base:Act ;
                                        rdfs:range base:DramaticExpression ;
                                        rdfs:comment "A certain act can be found in a particular dramatic expression" ;
                                        rdfs:label "act belongs to dramatic expression" .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:DatatypeProperty ;
              rdfs:domain frbr:work .


###  http://www.owl-ontologies.com/base#ActID
base:ActID rdf:type owl:DatatypeProperty ;
           rdfs:domain base:Act ;
           rdfs:range xsd:string ;
           rdfs:comment "Identification of the act of a Dramatic Work" ;
           rdfs:label "act i d" .


###  http://www.owl-ontologies.com/base#Analysis
base:Analysis rdf:type owl:DatatypeProperty ;
              rdfs:domain base:AccentAnalysis ;
              rdfs:range xsd:string ;
              rdfs:comment "String representation of an accentual Analysis of a verse" ;
              rdfs:label "analysis" .


###  http://www.owl-ontologies.com/base#AuthorId
base:AuthorId rdf:type owl:DatatypeProperty ;
              rdfs:domain base:Author ;
              rdfs:range xsd:int ;
              rdfs:comment "Identifier of an author" ;
              rdfs:label "author id" .


###  http://www.owl-ontologies.com/base#Birthday
base:Birthday rdf:type owl:DatatypeProperty ;
              rdfs:domain base:Author ;
              rdfs:comment "birthdate of an author" ;
              rdfs:label "birthdate" .


###  http://www.owl-ontologies.com/base#CompositionDate
base:CompositionDate rdf:type owl:DatatypeProperty ;
                     rdfs:domain frbr:work ;
                     rdfs:range xsd:dateTime ;
                     rdfs:comment "composition date of a dramatic work" ;
                     rdfs:label "composition date" .


###  http://www.owl-ontologies.com/base#Deathday
base:Deathday rdf:type owl:DatatypeProperty ;
              rdfs:domain base:Author ;
              rdfs:comment "deathdate of an author" ;
              rdfs:label "deathdate" .


###  http://www.owl-ontologies.com/base#DeweyCode
base:DeweyCode rdf:type owl:DatatypeProperty ;
               rdfs:domain frbr:expression ;
               rdfs:range xsd:string ;
               rdfs:comment "Dewey Code (bibliographic classification) assigned to a certain expression. It is compatible with most administrative systems in libraries all over the world." ;
               rdfs:label "dewey code" .


###  http://www.owl-ontologies.com/base#EditingEntityId
base:EditingEntityId rdf:type owl:DatatypeProperty ;
                     rdfs:domain base:EditingEntity ;
                     rdfs:range xsd:int ;
                     rdfs:comment "Identifier of an editing entity" ;
                     rdfs:label "editing entitiy id" .


###  http://www.owl-ontologies.com/base#HasPseudonym
base:HasPseudonym rdf:type owl:DatatypeProperty ;
                  rdfs:domain base:Character ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Fake name of a character, usually used to hide thier original identity" ;
                  rdfs:label "has pseudonym" .


###  http://www.owl-ontologies.com/base#Identificador
base:Identificador rdf:type owl:DatatypeProperty ;
                   rdfs:domain frbr:work ;
                   rdfs:range xsd:string ;
                   rdfs:comment "identificator of entities" ;
                   rdfs:label "identificator" .


###  http://www.owl-ontologies.com/base#Localization
base:Localization rdf:type owl:DatatypeProperty ;
                  rdfs:domain base:Scene ;
                  rdfs:range xsd:string ;
                  rdfs:comment "localization in which a scene takes place" ;
                  rdfs:label "localization" .


###  http://www.owl-ontologies.com/base#MetreID
base:MetreID rdf:type owl:DatatypeProperty ;
             rdfs:domain base:Metre ;
             rdfs:range xsd:string ;
             rdfs:comment "Identification of a certain metre" ;
             rdfs:label "metre i d" .


###  http://www.owl-ontologies.com/base#MetreSchema
base:MetreSchema rdf:type owl:DatatypeProperty ;
                 rdfs:domain base:Metre ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Formal and metric pattern followed by a metre schema" ;
                 rdfs:label "metre schema" .


###  http://www.owl-ontologies.com/base#MetricEntityID
base:MetricEntityID rdf:type owl:DatatypeProperty ;
                    rdfs:domain base:MetricEntity ;
                    rdfs:range xsd:string ;
                    rdfs:comment "metrici entity identifier" ;
                    rdfs:label "metric entity id" .


###  http://www.owl-ontologies.com/base#NSyllables
base:NSyllables rdf:type owl:DatatypeProperty ;
                rdfs:domain base:Verse ;
                rdfs:range xsd:int ;
                rdfs:comment "Number of syllables of a verse" ;
                rdfs:label "n syllables" .


###  http://www.owl-ontologies.com/base#Name
base:Name rdf:type owl:DatatypeProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( base:Author
                                      base:Character
                                    )
                      ] ;
          rdfs:range xsd:string ;
          rdfs:comment "name of a person or character" ;
          rdfs:label "name" .


###  http://www.owl-ontologies.com/base#PublicationDate
base:PublicationDate rdf:type owl:DatatypeProperty ;
                     rdfs:domain frbr:expression ;
                     rdfs:comment "Date of publication of a given expression" ;
                     rdfs:label "publication date" .


###  http://www.owl-ontologies.com/base#ReleaseDate
